a = 0
b = 1
for s in range(0, 10):
    for z in range(0, 3):
        if a % 1 == 0.0:
            print('I={:.0f} J={:.0f}'.format(a, b), end = '\n')
        elif a % 1 != 0:
            print('I={:.1f} J={:.1f}'.format(a, b), end='\n')
        b = b + 1
    a = a + 0.2
    b = b - 3
    b = b + 0.2
for z in range(0, 3):
    print('I={:.0f} J={:.0f}'.format(a, b), end = '\n')
    b = b + 1
