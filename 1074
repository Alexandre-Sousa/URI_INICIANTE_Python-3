val = int(input())

for i in range(1, val + 1, 1):
    a = int(input())
    if ((a % 2) == 0) and (a > 0):
        print('EVEN POSITIVE')
    elif ((a % 2) == 0) and (a < 0):
        print('EVEN NEGATIVE')
    elif ((a % 2) != 0) and (a > 0):
        print('ODD POSITIVE')
    elif ((a % 2) != 0) and (a < 0):
        print('ODD NEGATIVE')
    else:
        print('NULL')
