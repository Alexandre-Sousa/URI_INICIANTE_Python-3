x = 1
y = 1
while (x != 0) or (y != 0):
    val = input()
    x, y = val.split()
    x = int(x)
    y = int(y)
    if x == 0 or y == 0:
        break
    if x > 0 and y > 0:
        print('primeiro', end = '\n')
    elif x < 0 and y > 0:
        print('segundo', end = '\n')
    elif x < 0 and y < 0:
        print('terceiro', end = '\n')
    else:
        print('quarto', end = '\n')
