val = input()
a, b, c = val.split(' ')
lista = []
a = int(a)
b = int(b)
c = int(c)
lista.append(a)
lista.append(b)
lista.append(c)
listaord = sorted(lista)
print('{}'.format(listaord[0]))
print('{}'.format(listaord[1]))
print('{}'.format(listaord[2]))
print('')
print('{}'.format(lista[0]))
print('{}'.format(lista[1]))
print('{}'.format(lista[2]), end = '\n')
