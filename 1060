a = float(input())
b = float(input())
c = float(input())
d = float(input())
e = float(input())
f = float(input())
cont = 0

if a > 0:
    cont = cont +1
if b > 0:
    cont = cont +1
if c > 0:
    cont = cont +1
if d > 0:
    cont = cont +1
if e > 0:
    cont = cont +1
if f > 0:
    cont = cont +1

print('{} valores positivos'.format(cont), end = '\n')
